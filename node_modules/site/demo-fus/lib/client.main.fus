fus 2.4.1
import "./shared.manifest" all

import "./shared.main"
client: import "site/client"
lit: client.lit
ui: client.ui
ui.setRem(0.03)
ui.root.add ui.Stack(
    {scale: 0}
    ui.Text({fontSize: 1.6}, lit.nodeVersionTitle)
    ui.Text() as nodeVersionText
) as stack
setImmediate <>
    stack.setTransition(800).setScale(1)
do <>
    nodeVersionText.setSource web.jsonGet("/node/version")'wait.body
